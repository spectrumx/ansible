[connection]
id=MEP-{{ hostname }}
type=wifi
interface-name=wlan0
autoconnect-priority=1

[wifi]
mode=ap
ssid=MEP-{{ hostname }}

[wifi-security]
group=ccmp;
key-mgmt=sae
pairwise=ccmp;
proto=rsn;
psk=meppassword

[ipv4]
address1=192.168.44.1/24
may-fail=false
method=shared
never-default=true

[ipv6]
addr-gen-mode=stable-privacy
method=ignore

[proxy]
